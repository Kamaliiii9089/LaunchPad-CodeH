{
  "env": {
    "node": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-unused-vars": ["warn", { "argsIgnorePattern": "^_|next" }],
    "no-console": "off"
  },
  "globals": {
    "process": "readonly",
    "__dirname": "readonly",
    "__filename": "readonly",
    "module": "readonly",
    "require": "readonly",
    "exports": "readonly"
  }
}
